# 데이터베이스 설정
DATABASE_URL="postgresql://bookmark_user:bookmark_password@localhost:5432/bookmark_manager"

# 테스트 데이터베이스 설정
TEST_DATABASE_URL="postgresql://bookmark_user:bookmark_password@localhost:5433/bookmark_manager_test"

# JWT 설정
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_EXPIRES_IN="24h"

# 서버 설정
PORT=3000
NODE_ENV="development"

# 로그 설정
LOG_LEVEL="info"

# CORS 설정
CORS_ORIGIN="http://localhost:3000"

# Rate Limiting 설정
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=900000

# 웹사이트 메타데이터 추출 설정
METADATA_FETCH_TIMEOUT=5000

# 개발 환경 설정
DEV_PRETTY_LOGS=true